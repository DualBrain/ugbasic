0022A3r 1               
0022A3r 1               ; VIC1LINES = 312
0022A3r 1               ; VIC1CYCLESPERLINE = 71
0022A3r 1               ; VIC1TIMERVALUE = ( VIC1LINES * VIC1CYCLESPERLINE - 2 ) / 4
0022A3r 1               ; VIC1BANK:       .BYTE 1
0022A3r 1               VIC1IRQ:
0022A3r 1               ; VIC1IRQX2:
0022A3r 1               ; ;   lda $9114     ; get the NMI timer A value
0022A3r 1               ; ;                 ; (42 to 49 cycles delay at this stage)
0022A3r 1               ; ; ; sta $1e00     ; uncomment these if you want to monitor
0022A3r 1               ; ; ; ldy $9115     ; the reference timer on the screen
0022A3r 1               ; ; ; sty $1e01
0022A3r 1               ; ;   cmp #8        ; are we more than 7 cycles ahead of time?
0022A3r 1               ; ;   bcc VIC1IRQX0
0022A3r 1               ; ;   pha           ; yes, spend 8 extra cycles
0022A3r 1               ; ;   pla
0022A3r 1               ; ;   and #7        ; and reset the high bit
0022A3r 1               ; ; VIC1IRQX0:
0022A3r 1               ; ;   cmp #4
0022A3r 1               ; ;   bcc VIC1IRQX1
0022A3r 1               ; ;   bit $24       ; waste 4 cycles
0022A3r 1               ; ;   and #3
0022A3r 1               ; ; VIC1IRQX1:
0022A3r 1               ;   ; cmp #2        ; spend the rest of the cycles
0022A3r 1               ;   ; bcs *+2
0022A3r 1               ;   ; bcs *+2
0022A3r 1               ;   ; lsr
0022A3r 1               ;   ; bcs *+2       ; now it has taken 82 cycles from the beginning of the IRQ
0022A3r 1               ; ;     LDA VIC1BANK
0022A3r 1               ; ;     BEQ VIC1IRQL4A
0022A3r 1               ; ; VIC1IRQL4B:
0022A3r 1               ; ;     LDA $9004
0022A3r 1               ; ;     CMP #82
0022A3r 1               ; ;     BCS VIC1IRQEX
0022A3r 1               ; ; VIC1IRQL4B1:
0022A3r 1               ; ;     LDA $9004
0022A3r 1               ; ;     CMP #82
0022A3r 1               ; ;     BNE VIC1IRQL4B1
0022A3r 1               ; ;     LDA #$CF
0022A3r 1               ; ;     STA $9005
0022A3r 1               ; ;     LDA #6
0022A3r 1               ; ;     STA $900F
0022A3r 1               ; ;     LDA #0
0022A3r 1               ; ;     STA VIC1BANK
0022A3r 1               ; ;     JMP VIC1IRQEX
0022A3r 1               ; ; VIC1IRQL4A:
0022A3r 1               ; ;     LDA $9004
0022A3r 1               ; ;     CMP #0
0022A3r 1               ; ;     BNE VIC1IRQL4A
0022A3r 1               ; ;     LDA #$CD
0022A3r 1               ; ;     STA $9005
0022A3r 1               ; ;     LDA #2
0022A3r 1               ; ;     STA $900F
0022A3r 1               ; ;     LDA #1
0022A3r 1               ; ;     STA VIC1BANK
0022A3r 1               ; ;     JMP VIC1IRQEX
0022A3r 1               ; ; VIC1IRQEX:
0022A3r 1  20 rr rr     	JSR MUSICPLAYER
0022A6r 1  4C BF EA     	JMP $EABF
