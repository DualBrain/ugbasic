0022BBr 1               ;     LDA #$7f
0022BBr 1               ;     STA $912e     ; disable and acknowledge interrupts
0022BBr 1               ;     STA $912d
0022BBr 1               ;     STA $911e     ; disable NMIs (Restore key)
0022BBr 1               ; ;synchronize with the screen
0022BBr 1               ; VIC1STARTUPSYNC:
0022BBr 1               ;     LDX #28       ; wait for this raster line (times 2)
0022BBr 1               ; VIC1STARTUPSYNC2:
0022BBr 1               ;     CPX $9004
0022BBr 1               ;     BNE VIC1STARTUPSYNC2
0022BBr 1               ;                 ; at this stage, the inaccuracy is 7 clock cycles
0022BBr 1               ;                 ; the processor is in this place 2 to 9 cycles
0022BBr 1               ;                 ; after $9004 has changed
0022BBr 1               ;     LDY #9
0022BBr 1               ;     BIT $24
0022BBr 1               ; VIC1STARTUPSYNC3:
0022BBr 1               ;     LDX $9004
0022BBr 1               ;     TXA
0022BBr 1               ;     BIT $24
0022BBr 1               ;     LDX #24
0022BBr 1               ; VIC1STARTUPSYNC4:
0022BBr 1               ;     DEX
0022BBr 1               ;     BNE VIC1STARTUPSYNC4
0022BBr 1               ;                     ; first spend some time (so that the whole
0022BBr 1               ;     CMP $9004       ; loop will be 2 raster lines)
0022BBr 1               ;     BCS *+2       ; save one cycle if $9004 changed too late
0022BBr 1               ;     DEY
0022BBr 1               ;     BNE VIC1STARTUPSYNC3
0022BBr 1               ; VIC1STARTUPSYNC5:
0022BBr 1               ;                 ; now it is fully synchronized
0022BBr 1               ;                 ; 6 cycles have passed since last $9004 change
0022BBr 1               ;                 ; and we are on line 2(28+9)=74
0022BBr 1               ; VIC1STARTUPTIMERS:
0022BBr 1               ;     LDA #$40      ; enable Timer A free run of both VIAs
0022BBr 1               ;     STA $911b
0022BBr 1               ;     STA $912b
0022BBr 1               ;     LDA #<VIC1TIMERVALUE
0022BBr 1               ;     LDX #>VIC1TIMERVALUE
0022BBr 1               ;     STA $9116     ; load the timer low byte latches
0022BBr 1               ;     STA $9126
0022BBr 1               ;     LDY #7
0022BBr 1               ; VIC1STARTUPTIMERS2:
0022BBr 1               ;     DEY
0022BBr 1               ;     BNE VIC1STARTUPTIMERS2
0022BBr 1               ;     NOP
0022BBr 1               ;     NOP
0022BBr 1               ;     STX $9125
0022BBr 1               ;                 ; start the IRQ timer A
0022BBr 1               ;                 ; 6560-101: 65 cycles from $9004 change
0022BBr 1               ;                 ; 6561-101: 77 cycles from $9004 change
0022BBr 1               ;     LDY #10
0022BBr 1               ;                 ; spend some time (1+5*9+4=55 cycles)
0022BBr 1               ; VIC1STARTUPTIMERS3:
0022BBr 1               ;     DEY
0022BBr 1               ;                 ; before starting the reference timer
0022BBr 1               ;     BNE VIC1STARTUPTIMERS3
0022BBr 1               ;     STX $9115     ; start the reference timer
