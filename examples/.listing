   206                          ;*                                                                             *
   207                          ;*                         STARTUP ROUTINE FOR VDC                             *
   208                          ;*                                                                             *
   209                          ;*                             by Marco Spedaletti                             *
   210                          ;*                                                                             *
   211                          ;* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
   212                          ; The RAM used by the VDCZ is isolated from the main C128 memory map.
   213                          ; It can only be accessed through two registers that are mapped to $D600
   214                          ; (address register) and $D601 (data register).
   215                          VDCZADDRREG:  EQU     $D600
   216                          VDCZDATAREG:  EQU     $D601
   217                          ; Early C128 machines came with 16K of VDCZ RAM, expandable to 64K by added
   218                          ; three additional 16K chips. Later models (C128, C128CR) had 64K RAM. The
   219                          ; VDCZ is configured to use 16K or 64K modules, and we can check that configuration
   220                          ; by reading bit 4 of register 28:
   221                          ;
   222                          ;  :SetBankConfiguration(15)  // set bank 15
   223                          ;  ldx #28
   224                          ;  jsr READ_VDCZ
   225                          ;  sta $fb
   226                          ;  lda #010000
   227                          ;  bit $fb
   228                          ;  bne small
   229                          ;  lda #$31
   230                          ;  jsr $ffd2
   231                          ;  rts
   232                          ; small:
   233                          ;  lda #$30
   234                          ;  jsr $ffd2
   235                          ;  rts
   236                          ; The following procedures must be used to read/write from the VDCZ RAM:
   237                          ;
   238                          ;  - put the required register # in the address register;
   239                          ;  - wait for the ready bit in the address register to go high;
   240                          ;  - store or load the value from the data register;
   241                          ; Write a value to a VDCZ register
   242                          ;   - IXH : register
   243                          ;   - IXL : value
   244                          VDCZWRITE:
   245  007d  f5                	PUSH AF
   246  007e  c5                	PUSH BC
   247  007f  dd7c              	LD A, IXH
   248                              ; LD (VDCZADDRREG), A
   249  0081  0100d6            	LD BC, VDCZADDRREG
   250  0084  ed79              	OUT (C), A
   251                          VDCZWRITEL1:
   252                              ; LD A, (VDCZADDRREG)
   253  0086  ed78              	IN A, (C)
   254  0088  e680              	AND $80
   255  008a  fe00              	CMP $0
   256  008c  28f8              	JR Z, VDCZWRITEL1
   257  008e  dd7d              	LD A, IXL
   258                              ; LD (VDCZDATAREG), A
   259  0090  0101d6            	LD BC, VDCZDATAREG
   260  0093  ed79              	OUT (C), A
   261  0095  c1                	POP BC
   262  0096  f1                	POP AF
   263  0097  c9                	RET
   264                          ; Read a value from a VDCZ register
   265                          ;   - IXH : register
   266                          ;   - A : value
   267                          VDCZREAD:
   268  0098  c5                	PUSH BC
   269  0099  dd7c              	LD A, IXH
   270                              ; LD (VDCZADDRREG), A
   271  009b  0100d6            	LD BC, VDCZADDRREG
   272  009e  ed79              	OUT (C), A
   273                          VDCZREADL1:
   274                              ; LD A, (VDCZADDRREG)
   275  00a0  ed78              	IN A, (C)
   276  00a2  e680              	AND $80
   277  00a4  fe00              	CMP $0
   278  00a6  28f8              	JR Z, VDCZREADL1
   279                              ; LD A, (VDCZDATAREG)
   280  00a8  0101d6            	LD BC, VDCZDATAREG
   281  00ab  ed78              	IN A, (C)
   282  00ad  c1                	POP BC
   283  00ae  c9                	RET
   284                          ; Writing to the VDCZ RAM takes two steps:
   285                          ;  - Set the update address where you want to write to;
   286                          ;  - Pass data through the internal data register of the VDCZ (register 31);
   287                          ;
   288                          ; This is the macro to set the update address:
   289                          ;   - IY : address to set
   290                          VDCZSETADDR:
   291  00af  f5                	PUSH AF
   292  00b0  3e13              	LD A, 19
   293  00b2  dd67              	LD IXH, A
   294  00b4  fd7d              	LD A, IYL
   295  00b6  dd6f              	LD IXL, A
   296  00b8  cd7d00            	CALL VDCZWRITE
   297  00bb  3e12              	LD A, 18
   298  00bd  dd67              	LD IXH, A
   299  00bf  fd7c              	LD A, IYH
   300  00c1  dd6f              	LD IXL, A
   301  00c3  cd7d00            	CALL VDCZWRITE
   302  00c6  f1                	POP AF
   303  00c7  c9                	RET
   304                          ; Matrix Data
   305                          ;
   306                          ; The Matrix data area in the VDCZ is located at $0000-$07FF by default.
   307                          ; It's function is the same as screen memory in the C128 main memory map
   308                          ; ($0400 by default).  As we have 80 characters in a row, it takes
   309                          ; 2000 bytes, as opposed to the 1000 bytes on the 40 column display.
   310                          ; The address in VDCZ RAM can be retrieved by reading registers 12 and 13.
   311                          ; You can also change the address of the matrix data by writing a new value
   312                          ; to these registers. This macro will read the current setting, and store
   313                          ; it in $FB and $FC:
   314                          ;
   315                          ; Get VDCZ display start
   316                          ;   - IY : display
   317                          VDCZGETDISPLAYDSTART:
   318  00c8  dd260c            	LD IXH, 12
   319  00cb  cd9800            	CALL VDCZREAD
   320  00ce  fd67              	LD IYH, A
   321                          
   322  00d0  dd260d            	LD IXH, 13
   323  00d3  cd9800            	CALL VDCZREAD
   324  00d6  fd6f              	LD IYL, A
   325  00d8  c9                	RET
   326                          ; Writing to this RAM:
   327                          ;   - DE : address
   328                          ;   - A : value
   329                          VDCZPUTCHAR:
   330  00d9  fde5              	PUSH IY
   331  00db  d5                	PUSH DE
   332  00dc  fde1              	POP IY
   333  00de  cdaf00            	CALL VDCZSETADDR
   334  00e1  dd261f            	LD IXH, 31
   335  00e4  dd6f              	LD IXL, A
   336  00e6  cd7d00            	CALL VDCZWRITE
   337  00e9  fde1              	POP IY
   338  00eb  c9                	RET
   339                          ; Reading from this RAM:
   340                          ;   - DE : address
   341                          ;   - A : value
   342                          VDCZGETCHAR:
   343  00ec  fde5              	PUSH IY
   344  00ee  d5                	PUSH DE
   345  00ef  fde1              	POP IY
   346  00f1  cdaf00            	CALL VDCZSETADDR
   347  00f4  dd261f            	LD IXH, 31
   348  00f7  cd9800            	CALL VDCZREAD
   349  00fa  fde1              	POP IY
   350  00fc  c9                	RET
   351                          VDCZSTARTUP:
   352  00fd  c9                	RET
   353                          VDCZVIDEOSTARTUP:
   354  00fe  c9                	RET
